package Mediator;

public class Utilizator {
    private String name;

    public String getName(){
        return name;
    }
    public void setName(String name){
        this.name=name;
    }
    public Utilizator(String name){
        this.name=name;
    }
    public void expediazaMess(String mesaj){
        Messenger.accesMesaj(this, mesaj);
    }
}
